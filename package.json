{
  "name": "@desmoslabs/dapp-sdk",
  "private": true,
  "description": "",
  "author": "Manuel Turetta <manuel@forbole.com>",
  "license": "Apache-2.0",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "files": [
    "build/",
    "*.md",
    "!*.spec.*",
    "!**/testdata/"
  ],
  "workspaces": [
    "packages/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/desmos-labs/dapp-sdk"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "scripts": {
    "build": "yarn build-core && yarn build-react",
    "build-core": "yarn workspace @desmoslabs/sdk-core build",
    "build-react": "yarn workspace @desmoslabs/sdk-react build",
    "publish-next": "yarn workspaces foreach --no-private exec yarn npm publish --tag next",
    "publish-latest": "yarn workspaces foreach -A --no-private exec yarn npm publish"
  },
  "devDependencies": {
    "@cosmjs/crypto": "0.26.0",
    "@cosmjs/encoding": "0.26.0",
    "@cosmjs/proto-signing": "0.26.0",
    "@cosmjs/utils": "0.26.0",
    "@desmoslabs/sdk-core": "workspace:packages/sdk-core",
    "@desmoslabs/sdk-react": "workspace:packages/sdk-react",
    "@types/long": "4.0.1",
    "@types/pino": "6.3.11",
    "@walletconnect/jsonrpc-types": "1.0.0",
    "ts-proto": "~1.82.5",
    "typescript": "~4.3"
  }
}
